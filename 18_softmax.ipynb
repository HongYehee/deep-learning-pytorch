   "source": [
    "# 필요한 라이브러리 호출\n",
    "import numpy as np\n",
    "import math"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# softmax 함수 정의\n",
    "def softmax(x):\n",
    "    e = np.exp(x)\n",
    "    print(e)\n",
    "    print('=====')\n",
    "    print(e / e.sum())"
   ]
  },
  {
   "cell_type": "code",
